    "def reverse(s):\n",
    "   str = \"\"\n",
    "   for i in s:\n",
    "       str = i + str\n",
    "   return  str \n",
    "\n",
    "s = \"1234abcd\"\n",
    "\n",
    "print(\"the reversed string is :\",end=\"\")\n",
    "print(reverse(s))"
